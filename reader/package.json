  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-winston": "^3.1.0",
    "fs-extra": "^7.0.1",
    "node-fetch": "^2.6.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/express-winston": "^3.0.1",
    "@types/fs-extra": "^7.0.0",
    "@types/node": "^10.14.10",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "fetch-mock": "^7.3.1",
    "istanbul": "^0.4.5",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "source-map-support": "^0.5.12",
    "supertest": "^3.4.2",
    "tape": "^4.10.2",
