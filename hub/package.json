{
  "name": "gaia-hub",
  "version": "2.6.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@azure/storage-blob": "^10.3.0",
    "@google-cloud/storage": "^2.5.0",
    "ajv": "^6.10.0",
    "aws-sdk": "^2.482.0",
    "bitcoinjs-lib": "^5.1.2",
    "blockstack": "^19.2.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^3.2.1",
    "fs-extra": "^8.1.0",
    "jsontokens": "^2.0.2",
    "lru-cache": "^5.1.1",
    "nanoid": "^2.0.3",
    "node-fetch": "^2.6.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/express-winston": "^3.0.3",
    "@types/fetch-mock": "^7.3.1",
    "@types/fs-extra": "^8.0.0",
    "@types/lru-cache": "^5.1.0",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^10.14.10",
    "@types/node-fetch": "^2.3.7",
    "@types/proxyquire": "^1.3.28",
    "@types/supertest": "^2.0.7",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^5.0.0",
    "fetch-mock": "^7.3.3",
    "istanbul": "^0.4.5",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "source-map-support": "^0.5.12",
    "supertest": "^3.4.2",
    "tape": "^4.10.2",
    "tape-promise": "^4.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "typescript-json-schema": "^0.37.0"
  },
  "bin": {
    "blockstack-gaia-hub": "./lib/index.js"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc && chmod +x lib/index.js && npm run build-schema",
    "build-schema": "typescript-json-schema tsconfig.json HubConfig --required --noExtraProps --refs=false -o config-schema.json",
    "lint": "eslint --ext .ts ./src -f unix",
    "test": "npm run build && npm run lint && NODE_ENV=test nyc node ./test/src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blockstack/gaia.git"
  },
  "authors": [
    "Aaron Blankstein (aaron@blockstack.com)",
    "Jack Zampolin (jack@blockstack.com)",
    "Jude Nelson (jude@blockstack.com)"
  ],
  "nyc": {
    "cache": false,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
